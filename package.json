{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "npx prisma generate && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "start": "node -r dotenv/config build",
    "migrate": "prisma migrate dev",
    "sync": "npx svelte-kit sync"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.7",
    "@iconify/svelte": "^4.0.1",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.30.4",
    "@types/node": "^20.14.9",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.14.1",
    "autoprefixer": "^10.4.14",
    "classnames": "^2.5.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.34.1",
    "postcss": "^8.4.27",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "prisma": "5.16.1",
    "svelte": "^4.2.17",
    "svelte-check": "^3.4.3",
    "svelte-highlight": "^7.3.0",
    "svelte-markdown": "^0.4.1",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.4.5",
    "uuid": "^9.0.0",
    "vite": "^4.4.2",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2"
  },
  "overrides": {
    "svelte-markdown": {
      "svelte": "^4.0.0"
    }
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.6",
    "@gleam-lang/highlight.js-gleam": "^1.5.0",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "5.13.0",
    "@sentry/node": "^8.13.0",
    "@sentry/profiling-node": "^7.112.2",
    "@sentry/sveltekit": "^7.84.0",
    "@skeletonlabs/skeleton": "^2.10.1",
    "@skeletonlabs/tw-plugin": "^0.4.0",
    "@types/redis": "^4.0.11",
    "arctic": "^1.8.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "gray-matter": "^4.0.3",
    "lucia": "^3.2.0",
    "marked": "5",
    "nanoid": "^5.0.7",
    "reading-time": "^1.5.0",
    "redis": "^4.6.14"
  }
}
